%.o: %.c
	@echo '$(C_COMPILE) -c $<'; \
	$(C_COMPILE) $(C_DEPENDENCIES) -o $@ -c $<

01: $(01_OBJ)
	$(C_LINK) -o $@ $^ $(LDDEPS)

