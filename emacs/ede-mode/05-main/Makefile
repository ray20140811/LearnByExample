top="$(CURDIR)"/
ede_FILES=Project.ede Makefile

VERSION=1.0
DISTDIR=$(top)myproject-$(VERSION)



all: ch02 ch01

.PHONY:ch02
ch02:
	$(MAKE) -C ch02

.PHONY:ch01
ch01:
	$(MAKE) -C ch01

tags:
	$(MAKE) -C ch02/ $(MFLAGS) $@
	$(MAKE) -C ch01/ $(MFLAGS) $@

.PHONY: dist

dist:
	rm -rf $(DISTDIR)
	mkdir $(DISTDIR)
	cp $(ede_FILES) $(DISTDIR)
	$(MAKE) -C ch02 $(MFLAGS) DISTDIR=$(DISTDIR)/ch02 dist
	$(MAKE) -C ch01 $(MFLAGS) DISTDIR=$(DISTDIR)/ch01 dist
	tar -cvzf $(DISTDIR).tar.gz $(DISTDIR)
	rm -rf $(DISTDIR)

Makefile: Project.ede
	@echo Makefile is out of date!  It needs to be regenerated by EDE.
	@echo If you have not modified Project.ede, you can use ‘touch’ to update the Makefile time stamp.
	@false



