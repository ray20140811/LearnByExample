CFLAGS=-I../include

top=../
ede_FILES=Project.ede Makefile

myprogram_SOURCES=main.c mylib.c
myprogram_OBJ= main.o mylib.o
CC=gcc
C_COMPILE=$(CC) $(DEFS) $(INCLUDES) $(CPPFLAGS) $(CFLAGS)
C_DEPENDENCIES=-Wp,-MD,.deps/$(*F).P
C_LINK=$(CC) $(CFLAGS) $(LDFLAGS) -L.
VERSION=1.0
DISTDIR=$(top)myproject-$(VERSION)/src
top_builddir = ..

DEP_FILES=.deps/main.P .deps/mylib.P

all: myprogram

DEPS_MAGIC := $(shell mkdir .deps > /dev/null 2>&1 || :)
-include $(DEP_FILES)

%.o: %.c
	@echo '$(C_COMPILE) -c $<'; \
	$(C_COMPILE) $(C_DEPENDENCIES) -o $@ -c $<

myprogram: $(myprogram_OBJ)
	$(C_LINK) -o $@ $^ $(LDDEPS)

tags:


clean:
	rm -f *.mod *.o *.obj .deps/*.P .lo

.PHONY: dist

dist:
	mkdir $(DISTDIR)
	cp $(myprogram_SOURCES) $(ede_FILES) $(DISTDIR)

